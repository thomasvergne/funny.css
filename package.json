{
  "version": "0.1.7",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "dist/output.js",
    "src/index.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomasvergne/funny.css.git"
  },
  "scripts": {
    "build:css": "postcss src/base.css --base src --dir dist",
    "build:js": "yarn prejss-cli -f json dist/base.css",
    "build": "yarn build:css && yarn build:js",
    "compile": "yarn build && yarn tailwindcss -c preview/tailwind.config.js -o preview/tailwind.css",
    "build:publish": "yarn build && yarn publish --access public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "funny.css",
  "author": "thomasvergne",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "chalk": "^5.3.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nesting": "^11.2.2",
    "prejss-cli": "^0.3.3",
    "tailwindcss": "^3.2.7"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
